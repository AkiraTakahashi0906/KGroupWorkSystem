<UserControl x:Class="KGroupWorkSystem.Views.WorkOrderView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:prism="http://prismlibrary.com/"             
             prism:ViewModelLocator.AutoWireViewModel="True">
    <Grid>
        <StackPanel>
            <Label Content="作業指示作成"/>
            <Label Content= "{Binding TimeText.Value}"/>
            <Button Content="Start" Width="100" Command="{Binding StartButton}"/>
            <Button Content="Time" Width="100" Command="{Binding TimeButton}"/>
        </StackPanel>
    </Grid>
</UserControl>
